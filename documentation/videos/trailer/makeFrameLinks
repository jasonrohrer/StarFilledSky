frame=0

# Testing, just some title frames

for i in `seq 0 1`;
do
   echo "Making frame link $frame"

   # for each of 90 title frames (3 seconds of video at 30 frames per second)

   for f in `seq 0 89`
   do 
       
       ln titles/title_"$i"_"$f".jpg frames/frame_"$frame".jpg
       
       let "frame += 1"
   done
done